{
    "name": "vue",
    "displayName": "Vue Components",
    "description": ".vue component syntax highlighting for visual studio code",
    "version": "0.4.2",
    "author": "seanwash",
    "publisher": "seanwash",
    "icon": "images/logo.png",
    "scripts": {
        "release:minor": "vsce publish minor",
        "release:patch": "vsce publish patch"
    },
    "keywords": [
        "vue",
        "vuejs",
        "vue.js"
    ],
    "bugs": {
        "url": "https://github.com/Microsoft/vscode-spell-check/issues",
        "email": "hello@seanwash.com"
    },
    "homepage": "https://github.com/seanwash/vscode-vue-components/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/seanwash/vscode-vue-components"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "license": "MIT",
    "contributes": {
        "languages": [
            {
                "id": "vue",
                "aliases": [
                    "Vue Component",
                    "vue"
                ],
                "extensions": [
                    ".vue"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "vue",
                "scopeName": "text.html.vue",
                "path": "./syntaxes/vue.tmLanguage"
            }
        ]
    }
}